<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="Data Crow" location="applications"/>

  <skipIfNotSupported/>

  <shortcut
     name="Data Crow"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
     startup="no"
     target="java"
     commandLine="-jar &quot;$INSTALL_PATH/datacrow.jar&quot;"
     workingDirectory=""
     description="Data Crow"
     iconFile="$INSTALL_PATH/icons/datacrow64.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="true"
     KdeSubstUID="false"
     initialState="normal">   
   </shortcut>
   
   <shortcut
     name="Data Crow Readme"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\readme\readme.txt"
     workingDirectory="$INSTALL_PATH"
     commandLine=""
     description="Data Crow"
     initialState="normal"
     iconIndex="0">
   </shortcut>
   
   <shortcut
     name="Data Crow Change Log"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\readme\changelog.txt"
     workingDirectory="$INSTALL_PATH"
     commandLine=""
     description="Data Crow"
     initialState="normal"
     iconIndex="0">
   </shortcut>
   
</shortcuts>
